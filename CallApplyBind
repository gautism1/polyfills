CALL POLYFILL
 
  let obj ={
 name:"vipin",
 }
 function mymethod(a,b) 
    { 
            console.log(this.name,a,b)
     }

 let obj2={
 name:"hello"
 }
 
mymethod.call(obj);
 
 Function.prototype.mycall=function(...args)
{

let x=args[0];
x.fn=this;
let a=args.slice(1)
x.fn(a[0],a[1]);

} 
 mymethod.mycall(obj2,"hfhfh","jjjf");
 
 
 
 
 
 
